      replicas: 1
      selector:
        matchLabels:
          service: concord3
      strategy:
        type: Recreate
      template:
        metadata:
          labels:
            service: concord3
        spec:
          imagePullSecrets:
          - name: regcred-concord3
          containers:
            - image: concord_repo:concord_tag
              resources:
                requests:
                  memory: "2Gi"
                  cpu: "500m"
                limits:
                  memory: "2Gi"
                  cpu: "500m"
              imagePullPolicy: Always
              name: concord3
              ports:
                - containerPort: 5458
                - containerPort: 50051
                - containerPort: 3501
              resources: {}
              securityContext:
                privileged: true
              volumeMounts:
