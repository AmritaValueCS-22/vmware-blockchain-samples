apiVersion: v1
items:
  - apiVersion: v1
    kind: ServiceAccount
    metadata:
      name: concord3
  - apiVersion: rbac.authorization.k8s.io/v1
    kind: ClusterRole
    metadata:
      name: concord3
    rules:
    - apiGroups: [""]
      resources: ["services", "endpoints", "pods", "pods/log", "deployments", "configmaps", "secrets"]
      verbs: ["create", "get", "watch", "list", "update", "delete"]
  - apiVersion: rbac.authorization.k8s.io/v1
    kind: RoleBinding
    metadata:
      name: concord3
    subjects:
    - kind: ServiceAccount
      name: concord3
      apiGroup: ""
    roleRef:
      kind: ClusterRole
      name: concord3
      apiGroup: ""
  - apiVersion: v1
